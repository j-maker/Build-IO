            "thumbnailsOrientation": "vertical", //string - values: vertical, horizontal
            "zoomFactor": 2, //number 
            "zoomMode": "in-place-click", //string - values: in-place-click, disabled, in-place-hover, open-modal
            "ModalZoom": "in-place-click", //block - name of the block
            "contentType": "all" //string - values: all, images, videos
        }
    },
    "product-specification-badges#PDP": {
        "props": {
            "specificationGroupName": "Group", //string
            "specificationName": "On Sale", //string

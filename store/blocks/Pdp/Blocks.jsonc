{
    "breadcrumb#pdp": {
        "props": {
            "blockClass": "",
            "showOnMobile": false, //boolean
            "homeIconSize": 26, //number
            "caretIconSize": 8 //number
        }
    },
    "stack-layout#PDP": {
        "props": {
            "blockClass": "",
            "zIndexOffset": 0 //number
        },
        "children": [
            "flex-layout.row#product-image",
            "product-specification-badges#PDP"
        ]
    },
    "product-images#PDP": {
        "props": {
            "aspectRatio": "auto", //string - values: auto, 3:4, 1:1, 1920:1080
